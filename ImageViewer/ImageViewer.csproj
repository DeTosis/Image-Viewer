<Project Sdk="Microsoft.NET.Sdk">

  <PropertyGroup>
    <OutputType>WinExe</OutputType>
    <TargetFramework>net8.0-windows</TargetFramework>
    <Nullable>enable</Nullable>
    <ImplicitUsings>enable</ImplicitUsings>
    <UseWPF>true</UseWPF>
    <ApplicationIcon>IMG\pngOpener.ico</ApplicationIcon>
  </PropertyGroup>

  <ItemGroup>
    <None Remove="IMG\pngOpener.png" />
  </ItemGroup>

  <ItemGroup>
    <Content Include="IMG\pngOpener.ico" />
    <Content Include="IMG\pngOpener.png">
      <CopyToOutputDirectory>PreserveNewest</CopyToOutputDirectory>
    </Content>
  </ItemGroup>

</Project>
